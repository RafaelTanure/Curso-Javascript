// Um objeto dentro de javaScript é um conjunto de chave e valor, podemos ter um objeto dentro de outro objeto. Objeto é um grupo de pares chave valor.
const prod1 = {};
prod1.nome = ' Celular Ultra Mega ';
prod1.preco = 4998.90;
prod1['Desconto Legal'] = 0.40 // Evitar atributos com espaço 

console.log(prod1);

const prod2 = {
    nome: 'Camisa Polo',
    preco: 79.90,
    obj: {
        blabla: 1,
        //Podemos ter uma estrutura aninhada no objeto . 
        obj: {
            blabla: 2
        }
    }
}; 
console.log(prod2);

// Exemplo de Json 

'{"nome" : "Camisa Polo", "preco": 79.90}' // Não é a mesma coisa de objeto é um formato textual. Já Objeto é atributo é comportamento 
